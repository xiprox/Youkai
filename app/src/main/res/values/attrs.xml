<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BarInfoView">
        <attr name="biv_text" format="string" />
        <attr name="biv_icon" format="reference" />
        <attr name="biv_roundedCorners" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PreferenceView">
        <attr name="preferenceCheckableType" format="enum">
            <enum name="check_box" value="0" />
            <enum name="switch_compat" value="1" />
        </attr>
        <attr name="preferenceDisabledDescriptionText" format="string" />
        <attr name="preferenceEnabledTitleText" format="string" />
        <attr name="preferenceDescriptionText" format="string" />
        <attr name="preferenceStartEnabled" format="boolean" />
        <attr name="preferenceTitleText" format="string" />
    </declare-styleable>

</resources>
